package qwerty;
public class Chihuahua extends Dog{
    private String color;
    public Chihuahua(String nov, String mat){
        super(nov, mat);
    }
    public static void main(String[] args) {
        Dolmotinec posuda = new Dolmotinec("Редкая", "Оранжевая");
