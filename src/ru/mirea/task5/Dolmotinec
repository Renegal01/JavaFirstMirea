package qwerty;
public class Dolmotinec extends Dog{
    private String color;
    public Dolmotinec(String nov, String mat){
        super(nov, mat);
    }
    public static void main(String[] args) {
        Dolmotinec posuda = new Dolmotinec("Obichnaya", "cherno-belaya");
        System.out.println("Собака породистая?" + posuda.isporodyastaya() );
        System.out.println("A цвета какого?" + posuda.getraskraska());
    }
}
